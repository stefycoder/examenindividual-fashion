    header#logo
      .container
        .row
          .col-md-4
            h2 OPENING TIME
            span 9.00 AM-10.00 PM
          .col-md-4
            img(src="Problema/assets/images/logo.png", alt="logo")
          .col-md-4
            input(type="text",placeholder="search")
    header#compra
      .navbar.navbar-default.navbar-fixed(role='navigation')
        .container
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
          ul.nav.navbar-nav
            li.active
              a(href='index.html') HOME
            li
              a(href='contact.html') CONTACT US
    #myCarousel.carousel.slide(data-ride='carousel')
      ol.carousel-indicators
        li.active(data-target='#myCarousel', data-slide-to='0')
        li(data-target='#myCarousel', data-slide-to='1')
      .carousel-inner
        .item.active
          img(src='Problema/assets/images/slider1.jpg', alt='First slide')
          .container
            .carousel-caption
              h1 AUTUMN/WINTER LOOKBOOK
              p
                |-2016 COLLECTION-
        .item
          img(src='Problema/assets/images/slider2.jpg', alt='Second slide')
          .container
            .carousel-caption
              h1 AUTUMN/WINTER LOOKBOOK
              p
                |-2016 COLLECTION-
      a.left.carousel-control(href='#myCarousel', role='button', data-slide='prev')
        span.glyphicon.glyphicon-chevron-left
      a.right.carousel-control(href='#myCarousel', role='button', data-slide='next')
        span.glyphicon-chevron-right
